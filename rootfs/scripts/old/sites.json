{
  "hosts": {
    "api.anq.finance": {
      "root": "/app/anq.finance/", 
      "modsecurity": true, 
      "locations": {
        "/ping": "ping", 
        "/status": "status", 
        "/": {
            "upstream": "api.anq.finance_upstream"
        }
      }
    }
  },
  "upstreams": {
    "api.anq.finance_upstream": {
        "docker_service": "api_anq_finance",
        "port": 1234,
        "servers": {
            "10.10.0.1": {
            "weight": 1,
            "max_fails": 2,
            "fail_timeout": 30,
            "backup": false,
            "down": false,
            "port": 1234
            }
        }
    }
  }
}
